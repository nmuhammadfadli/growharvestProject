<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="100dp"
    android:background="@color/material_dynamic_neutral90">

    <TextView
        android:id="@+id/isiItem"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:text="Pestisida rmx"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/isiHarga"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:text="38000"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/isiItem" />

    <TextView
        android:id="@+id/isiKuantitas"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="144dp"
        android:layout_marginBottom="30dp"
        android:text="4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/isiHarga" />

    <TextView
        android:id="@+id/isiTotal"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="99dp"
        android:layout_marginTop="16dp"
        android:text="152000"
        app:layout_constraintStart_toEndOf="@+id/isiItem"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>